    <CheckBoxPreference android:persistent="false" android:title="@string/enable_http_proxy" android:key="enable_http_proxy" android:summary="@string/enable_http_proxy_summary" />
    <Preference android:persistent="false" android:title="@string/http_proxy_settings" android:key="http_proxy_settings" android:summary="@string/http_proxy_settings_summary" android:dependency="enable_http_proxy" />
    <Preference android:persistent="false" android:title="@string/http_buffer_size" android:key="http_buffer_size" android:summary="@string/http_buffer_size_summary" />
