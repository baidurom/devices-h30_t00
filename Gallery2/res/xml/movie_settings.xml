    <Preference android:persistent="false" android:title="@string/rtsp_proxy_settings" android:key="rtsp_proxy_settings" android:summary="@string/rtsp_proxy_settings_summary" android:dependency="enable_rtsp_proxy" />
    <Preference android:persistent="false" android:title="@string/udp_port_settings" android:key="udp_port_settings" android:summary="@string/udp_port_settings_summary" />
